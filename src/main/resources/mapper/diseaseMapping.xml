<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.dao.IDiseaseDao">

    <resultMap id="diseaseMap" type="DiseaseBean">
        <id column="id" property="id"></id>
        <result column="name" property="name"></result>
        <result column="pattern" property="pattern"></result>
        <result column="harm" property="harm"></result>
        <result column="cause" property="cause"></result>
        <result column="measure" property="measure"></result>
        <result column="symptom" property="symptom"></result>
        <result column="img" property="img"></result>
    </resultMap>

</mapper>

